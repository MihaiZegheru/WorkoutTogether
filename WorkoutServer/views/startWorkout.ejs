<script>
    document.getElementById("start-workout-button").onclick = startWorkout;

    function startWorkout() {
        try {
            const res = fetch(String(window.location.pathname), {
                method: 'POST',
                body: JSON.stringify({ numberOfExercise: "0" }),
                headers: { 'Content-Type': 'application/json' }
            });
        }
        catch (err) {
            console.log(err);
        }
        // send req to server to get this exrcise
    }

</script>

